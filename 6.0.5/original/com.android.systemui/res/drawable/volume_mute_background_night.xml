<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <solid android:color="#ff2c3038" />
            <corners android:radius="6.0px" />
            <stroke android:width="2.0px" android:color="#ffbe7153" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape>
            <solid android:color="#05ffffff" />
            <corners android:radius="6.0px" />
            <stroke android:width="2.0px" android:color="#99e1e3e5" />
        </shape>
    </item>
</selector>